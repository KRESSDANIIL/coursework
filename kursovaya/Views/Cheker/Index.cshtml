@{
    Layout = "_Layout";
}
@using Kursovaya.Models
@model Kursovaya.Models.ChekerViewModel

<h2>Проверить абонимент</h2>

<form asp-action="Insert">
    <div asp-validation-summary="ModelOnly" class="text-danger"></div>
    <div class="form-group">
        <label asp-for="MembershipType" class="control-label"></label>
        <input asp-for="MembershipType" class="form-control" />
        <span asp-validation-for="MembershipType" class="text-danger"></span>
    </div>
    <div class="form-group">
        <input type="submit" value="Check Sessions" class="btn btn-primary" />
    </div>
</form>